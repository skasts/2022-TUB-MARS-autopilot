<?xml version="1.0"?>
<sdf version='1.7'>
  <model name='mars_robot'>
    <pose>0 0 0.5 0 0 0</pose>
    <link name='mars_robot_footprint' />

    <link name='robot_base_link'>
      <visual name='robot_base_link_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
      <inertial>
        <pose>0.000112 2.3e-05 0.00015 0 -0 0</pose>
        <mass>4.43</mass>
        <inertia>
          <ixx>0.0330025</ixx>
          <ixy>-4.93883e-06</ixy>
          <ixz>-3.28432e-05</ixz>
          <iyy>0.0553027</iyy>
          <iyz>-6.63499e-06</iyz>
          <izz>0.0560386</izz>
        </inertia>
      </inertial>
      <collision name='robot_base_link_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.33 0.355 0.3</size>
          </box>
        </geometry>
      </collision>
    </link>

    <joint name="base_joint" type="fixed">   
        <parent link="mars_robot_footprint"/>
        <child link="robot_base_link"/>            
    </joint>

    <!--CAMERA-->
    <link name= "camera_link">
      <visual name='camera_link_visual'>
        <pose relative_to='camera_joint'> 0 0 0.2045 0 -0 0 />
        <geometry>
          <box>
            <size>0.128 0.084 0.061</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Orange</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
      <sensor name='camera' type='camera'>
        <update_rate>30</update_rate>
        <camera name='head'>
          <horizontal_fov>1.39626</horizontal_fov>
          <image>
            <width>800</width>
            <height>800</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name='camera_controller' filename='libgazebo_ros_camera.so'>
          <alwaysOn>1</alwaysOn>
          <updateRate>30.0</updateRate>
          <cameraName>mars_robot/camera</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>camera_link_optical</frameName>
          <hackBaseline>0.0</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
          <CxPrime>0</CxPrime>
          <Cx>0.0</Cx>
          <Cy>0.0</Cy>
          <focalLength>0.0</focalLength>
        </plugin>
        <pose>0 0 0.2045 0 -0 0</pose>
      </sensor>
  </link>

  <link name="camera_link_optical"></link>

  <joint name="camera_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.0 0 0.2045" />
        <parent link="robot_base_link" />
        <child link="camera_link" />
  </joint>

<!-- generate an optical frame http://www.ros.org/reps/rep-0103.html#suffix-frames
      so that ros and opencv can operate on the camera frame correctly -->
  <joint name="camera_optical_joint" type="fixed">
        <!-- these values have to be these values otherwise the gazebo camera image
        won't be aligned properly with the frame it is supposedly originating from -->
        <origin xyz="0 0 0" rpy="${-pi/2} 0 ${-pi/2}"/>
        <parent link="camera_link"/>
        <child link="camera_link_optical"/>
  </joint>

  <link name="imu_link">
      <inertial>
        <mass value="0.010" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="1e-09" ixy="0.0" ixz="0.0" iyy="1e-09" iyz="0.0" izz="1e-09" />
      </inertial>
      <gravity>1</gravity>
      <sensor name='imu_sensor' type='imu'>
        <always_on>1</always_on>
        <update_rate>100</update_rate>
        <visualize>1</visualize>
        <topic>__default_topic__</topic>
        <plugin name='imu_plugin' filename='libgazebo_ros_imu_sensor.so'>
          <topicName>imu</topicName>
          <bodyName>imu_link</bodyName>
          <updateRateHZ>10.0</updateRateHZ>
          <gaussianNoise>0.0</gaussianNoise>
          <xyzOffset>0 0 0</xyzOffset>
          <rpyOffset>0 0 0</rpyOffset>
          <frameName>imu_link</frameName>
          <initialOrientationAsReference>0</initialOrientationAsReference>
        </plugin>
        <pose>0.0495 0.01 0.0665 0 -0 0</pose>
      </sensor>
  </link>

  <joint name="imu_joint">
    <parent link="robot_base_link" />
        <child link="imu_link" />
        <origin rpy="0 0 0" xyz="0.0495 0.01 0.0665" />
  </joint>

  <joint name='front_left_wheel_joint' type='revolute'>
      <pose relative_to='robot_base_link'>0.13675 0.141 0 0 -0 0</pose>
      <parent>robot_base_link</parent>
      <child>front_left_wheel_link</child>
      <axis>
        <xyz>0 0 -1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
      <physics>
      <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.900000</erp>
          </limit>
        </ode>
      </physics>
  </joint>

<!--WHEELS-->
  <link name='front_left_wheel_link'>
    <pose relative_to='front_left_wheel_joint'>0 0 0 0 -0 0</pose>
    <inertial>
      <pose>0 0 0 0 -0 0</pose>
      <mass>0.176</mass>
      <inertia>
        <ixx>0.000287467</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000287467</iyy>
        <iyz>0</iyz>
        <izz>3.0184e-05</izz>
      </inertia>
    </inertial>
    <collision name='front_left_wheel_link_collision'>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>2</mu>
            <mu2>2</mu2>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name='front_left_wheel_link_visual'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Black</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
  </link>

  <joint name='front_right_wheel_joint' type='revolute'>
    <pose relative_to='robot_base_link'>0.13675 -0.141 0 0 -0 0</pose>
    <parent>robot_base_link</parent>
    <child>front_right_wheel_link</child>
    <axis>
        <xyz>0 0 -1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
      <physics>
      <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.900000</erp>
          </limit>
        </ode>
      </physics>
  </joint>

  <link name='front_right_wheel_link'>
    <pose relative_to='front_right_wheel_joint'>0 0 0 0 -0 0</pose>
    <inertial>
      <pose>0 0 0 0 -0 0</pose>
      <mass>0.176</mass>
      <inertia>
        <ixx>0.000287467</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000287467</iyy>
        <iyz>0</iyz>
        <izz>3.0184e-05</izz>
      </inertia>
    </inertial>
    <collision name='front_right_wheel_link_collision'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <surface>
        <contact>
          <ode/>
        </contact>
        <friction>
          <ode>
            <mu>2</mu>
            <mu2>2</mu2>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name='front_right_wheel_link_visual'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Black</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
  </link>

  <joint name='rear_left_wheel_joint' type='revolute'>
    <pose relative_to='mars_robot_footprint'>-0.05425 0.141 0 0 -0 0</pose>
    <parent>mars_robot_footprint</parent>
    <child>rear_left_wheel_link</child>
    <axis>
        <xyz>0 0 -1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
      <physics>
      <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.900000</erp>
          </limit>
        </ode>
      </physics>
  </joint>

  <link name='rear_left_wheel_link'>
    <pose relative_to='rear_left_wheel_joint'>0 0 0 0 -0 0</pose>
    <inertial>
      <pose>0 0 0 0 -0 0</pose>
      <mass>0.176</mass>
      <inertia>
        <ixx>0.000287467</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000287467</iyy>
        <iyz>0</iyz>
        <izz>3.0184e-05</izz>
      </inertia>
    </inertial>
    <collision name='rear_left_wheel_link_collision'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <surface>
        <contact>
          <ode/>
        </contact>
        <friction>
          <ode>
            <mu>2</mu>
            <mu2>2</mu2>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name='rear_left_wheel_link_visual'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Black</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
  </link>

  <joint name='rear_right_wheel_joint' type='revolute'>
    <pose relative_to='mars_robot_footprint'>-0.05425 -0.141 0 0 -0 0</pose>
    <parent>mars_robot_footprint</parent>
    <child>rear_right_wheel_link</child>
    <axis>
        <xyz>0 0 -1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
      <physics>
      <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.900000</erp>
          </limit>
        </ode>
      </physics>
  </joint>

  <link name='rear_right_wheel_link'>
    <pose relative_to='rear_right_wheel_joint'>0 0 0 0 -0 0</pose>
    <inertial>
      <pose>0 0 0 0 -0 0</pose>
      <mass>0.176</mass>
      <inertia>
        <ixx>0.000287467</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000287467</iyy>
        <iyz>0</iyz>
        <izz>3.0184e-05</izz>
      </inertia>
    </inertial>
    <collision name='rear_right_wheel_link_collision'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <surface>
        <contact>
          <ode/>
        </contact>
        <friction>
          <ode>
            <mu>2</mu>
            <mu2>2</mu2>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name='rear_right_wheel_link_visual'>
      <pose>0 0 0 0 -0 0</pose>
      <geometry>
        <cylinder>
          <length>0.07</length>
          <radius>0.125</radius>
        </cylinder>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Black</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
  </link>

<!-- Drive plugin -->
  <plugin name="mars_robot_drive_plugin" filename="libgazebo_ros_diff_drive.so">
    <update_rate>100.0</update_rate>
    <num_wheel_pairs>2</num_wheel_pairs>
    <left_joint>front_left_wheel_joint</left_joint>
    <right_joint>front_right_wheel_joint</right_joint>
    <left_joint>rear_left_wheel_joint</left_joint>
    <right_joint>rear_right_wheel_joint</right_joint>
    <wheel_separation>0.191</wheel_separation>
    <wheel_separation>0.191</wheel_separation>
    <wheel_diameter>0.246</wheel_diameter>
    <wheel_diameter>0.246</wheel_diameter>
    <max_wheel_torque>30</max_wheel_torque>
    <publish_odom>true</publish_odom>
    <publish_odom_tf>true</publish_odom_tf>
    <publish_wheel_tf>true</publish_wheel_tf>
    <odometry_frame>odom</odometry_frame>
    <robot_base_frame>mars_robot_footprint</robot_base_frame>
  </plugin>

  <static>0</static>
  <plugin name='gazebo_ros_control' filename='libgazebo_ros_control.so'>
    <robotNamespace>/mars_robot</robotNamespace>
    <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
  </plugin>
  </model>
</sdf>
